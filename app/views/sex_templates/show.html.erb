<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Sex #<%= @sex.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/sexes" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/sexes/<%= @sex.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit sex
        </a>
      </div>

      <div class="col">
        <a href="/delete_sex/<%= @sex.id %>" class="btn btn-block btn-outline-secondary">
          Delete sex
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Name
      </dt>
      <dd>
        <%= @sex.name %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@sex.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@sex.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "sex_templates/sex_patients" %>
  </div>
</div>
